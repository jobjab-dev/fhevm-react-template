{
  "buildCommand": "cd ../.. && pnpm install && pnpm sdk:build && pnpm next:build",
  "outputDirectory": "packages/nextjs/.next",
  "devCommand": "pnpm start",
  "installCommand": "npm install -g pnpm && pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_VERSION": "20.0.0",
    "ENABLE_EXPERIMENTAL_COREPACK": "1"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}


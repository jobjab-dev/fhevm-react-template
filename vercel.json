{
  "buildCommand": "pnpm install && pnpm sdk:build && pnpm next:build",
  "outputDirectory": "packages/nextjs/.next",
  "devCommand": "pnpm start",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NODE_VERSION": "20.0.0"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}


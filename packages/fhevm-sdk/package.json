{
  "name": "jobjab-fhevm-sdk",
  "version": "0.1.2",
  "description": "Universal, framework-agnostic SDK for building confidential dApps with FHEVM",
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "blockchain",
    "ethereum",
    "zama",
    "privacy",
    "confidential",
    "dapp",
    "react",
    "typescript"
  ],
  "author": "Zama Bounty Program Contributor",
  "repository": {
    "type": "git",
    "url": "https://github.com/jobjab-dev/fhevm-react-template"
  },
  "bugs": {
    "url": "https://github.com/jobjab-dev/fhevm-react-template/issues"
  },
  "homepage": "https://github.com/jobjab-dev/fhevm-react-template#readme",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./adapters/react": {
      "import": "./dist/adapters/react/index.js",
      "types": "./dist/adapters/react/index.d.ts"
    },
    "./components/react": {
      "import": "./dist/components/react/index.js",
      "types": "./dist/components/react/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "types": "./dist/storage/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "idb": "^8.0.3"
  },
  "peerDependencies": {
    "@fhevm/mock-utils": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "ethers": "^6.13.4",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "~18.19.50",
    "@types/react": "~19.0.7",
    "@vitest/coverage-v8": "2.1.9",
    "@zama-fhe/relayer-sdk": "0.2.0",
    "ethers": "^6.13.7",
    "fake-indexeddb": "~6.0.0",
    "jsdom": "^27.0.0",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "typescript": "~5.8.2",
    "vitest": "~2.1.8"
  }
}
